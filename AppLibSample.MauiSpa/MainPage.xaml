<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="VpnHood.App.AppLibSample.MauiSpa.MainPage">

    <Grid x:Name="MainLayout" HorizontalOptions="FillAndExpand" VerticalOptions="FillAndExpand">
        <WebView x:Name="MainWebView" Grid.Row="0" Grid.Column="0"></WebView>
        <Image x:Name="SplashScreen" IsVisible="true"
               Grid.Row="0" Grid.Column="0"
               Aspect="Center"
               BackgroundColor="{StaticResource Primary}"
               Source="splash.png"
               VerticalOptions="FillAndExpand"
               HorizontalOptions="FillAndExpand" />
    </Grid>
</ContentPage>
